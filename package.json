    "node-mic-record": "^0.0.1",
    "node-microphone": "^0.1.5",
    "prism-media": "^1.2.9",
    "recordrtc": "^5.6.2",
    "rezidb": "^1.0.1",
    "speaker": "^0.5.3",
    "ws": "^7.4.4",
    "ytdl-core": "^4.5.0",
    "ytdl-core-discord": "^1.3.0",
    "ytsr": "^3.3.1"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.19"
  }
