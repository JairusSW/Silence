{
  "name": "as-silence",
  "version": "1.0.0",
  "description": "Silence Chat/Voice App In Pure AssemblyScript",
  "main": "index.js",
  "bin": {
    "silence": "./test.js"
  },
  "scripts": {
    "install": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests",
    "asbuild:untouched": "asc assembly/index.ts --target debug --exportRuntime --exportTable",
    "asbuild:optimized": "asc assembly/index.ts --target release --exportRuntime --exportTable",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JairusSW/Silence.git"
  },
  "keywords": [
    "chat",
    "api",
    "udp",
    "websocket",
    "voice",
    "audio",
    "demo",
    "showcase"
  ],
  "author": "JairusSW",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JairusSW/Silence/issues"
  },
  "homepage": "https://github.com/JairusSW/Silence#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.18.19",
    "as-udp": "^1.0.0",
    "as-websocket": "^1.0.0",
    "assemblyscript": "^0.18.19",
    "audio-generator": "^2.1.2",
    "audio-speaker": "^1.5.1",
    "chalk": "^4.1.0",
    "discord.js": "^12.5.1",
    "electron": "^12.0.2",
    "fastify": "^3.14.1",
    "get-user-media-promise": "^1.1.4",
    "mic-stream": "^1.0.2",
    "microphone-stream": "^6.0.0",
